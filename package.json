  "license": "MIT",
  "scripts": {
    "setup": "npm i . && npm docker && npm db",
    "docker": "docker compose up -d",
    "db": "npx nx run prisma-schema:prisma generate",
    "db:seed": "npx nx run prisma-schema:seed",
    "lint:super-admin": "npx nx lint super-admin",
    "lint:admin": "npx nx lint admin",
    "build:super-admin": "npx nx build super-admin",
    "build:admin": "npx nx build admin",
    "test:super-admin": "npx nx test super-admin",
    "test:admin": "npx nx test admin",
    "format:super-admin": "npx nx format super-admin",
    "format:admin": "npx nx format admin",
    "start:super-admin": "npx nx serve super-admin",
    "start:admin": "npx nx serve admin",
    "start:all": "npx concurrently \"cross-env LOCAL_PORT=5430 npm run start:super-admin\" \"cross-env LOCAL_PORT=5431 npm run start:admin\"",
    "build:all": "npm run build:super-admin && npm run build:admin && rm -rf dist/output && mkdir dist/output && cp -r dist/apps/super-admin/* dist/output/ && cp -r dist/apps/admin/* dist/output/"
  },
  "prisma": {
    "schema": "./libs/db/prisma-schema/prisma/schema.prisma",
    "seed": "ts-node ./libs/db/prisma-schema/prisma/seed.ts"
  },
  "private": true,
